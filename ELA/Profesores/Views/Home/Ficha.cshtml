@model Profesor
<h1>Ficha</h1>
<fieldset class="form-horizontal">
    <legend class=" label"> Ficha Profesor</legend>
    <div class="form-group">
        <div class="label">Rut</div>
        <div class="text-muted">@Html.DisplayFor(m => m.rut)</div>
    </div>
    <div class="form-group">
        <div class="label">Nombre Completo</div>
        <div class="text-muted">@Html.DisplayFor(m => m.nombre)</div>
    </div>
    <div class="form-group">
        <div class="label">Título</div>
        <div class="text-muted">@Html.DisplayFor(m => m.titulo)</div>
    </div>
    <div class="form-group">
        <div class="label">Grado Profesional</div>
        <div class="text-muted">@Html.DisplayFor(m => m.grado)</div>
    </div>
</fieldset>
